<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.cloudera.archetype</groupId>
  <artifactId>mapred-driver-archetype</artifactId>
  <version>1.0-alpha-1</version>
  <packaging>maven-archetype</packaging>
  <name>mapred-driver-archetype</name>
  <build>
    <extensions>
      <extension>
        <groupId>org.apache.maven.archetype</groupId>
        <artifactId>archetype-packaging</artifactId>
        <version>2.0-alpha-4</version>
      </extension>
    </extensions>
    <plugins>
      <plugin>
        <artifactId>maven-archetype-plugin</artifactId>
        <version>2.0-alpha-4</version>
        <extensions>true</extensions>
      </plugin>
    </plugins>
  </build>
  <distributionManagement>
    <repository>
      <id>cloudera.releases.repo</id>
      <name>Cloudera Releases Repo</name>
      <url>https://repository.cloudera.com/releases</url>
    </repository>
    <snapshotRepository>
      <id>cloudera.snapshots.repo</id>
      <name>Cloudera Snapshots Repo</name>
      <url>https://repository.cloudera.com/snapshots</url>
    </snapshotRepository>
  </distributionManagement>
</project>
